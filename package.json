{
  "name": "auth-users",
  "version": "1.0.0",
  "main": "server.ts",
  "author": "Dustin",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^6.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.5",
    "concurrently": "^6.2.1",
    "husky": "^7.0.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client; yarn start"
  }
}
